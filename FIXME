(droll  0) thief ooze 

(delve=1, depth=3, ability=True, dungeon=(chest=1, potion=1), party=(cleric=1, thief=1, scroll=1), treasure=())
(droll  0) thief chest 

(delve=1, depth=3, ability=True, dungeon=(potion=1), party=(cleric=1, scroll=1), treasure=(talisman=1))
(droll  1) descend

(delve=1, depth=4, ability=True, dungeon=(skeleton=1, ooze=1, potion=2), party=(cleric=1, scroll=1), treasure=(talisman=1))
(droll  1) talisman ooze 

(delve=1, depth=4, ability=True, dungeon=(skeleton=1, potion=2), party=(scroll=1), treasure=(talisman=1))
(droll  1) talisman skeleton 

